package cl.bdly.controlador;

import javax.servlet.http.HttpServletResponse;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.servlet.ModelAndView;

import cl.bdly.servicio.CapacitacionService;

@Controller
public class CapacitacionController {
	
	@Autowired
	private CapacitacionService capaservice;
	
	@RequestMapping("/obtenercapacitaciones")
	public ModelAndView home(HttpServletResponse response) {
	    ModelAndView mav = new ModelAndView("listarcapas");
	    return mav;
	}
	
	

}
